[include]
	path = .gitconfig_local
[core]
	excludesfile = ~/.gitignore
	editor = vim
	quotepath = false
[color]
	ui = auto
[log]
	decorate = short
[push]
	default = current
[pull]
	rebase = true
[rebase]
	stat = true
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
